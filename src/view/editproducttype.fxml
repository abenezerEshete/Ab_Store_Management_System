                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <JFXTextField fx:id="sellingPrice" prefWidth="200.0" style="-fx-background-color: #EBF5FB;">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </JFXTextField>
                        <RequiredFieldValidator fx:id="sellingPriceRequierValidator" message="This field is required"/>
                        <DoubleValidator fx:id="sellingPriceNumberValidator"/>
                    </children>
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="30.0"/>
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="30.0" spacing="11.0">
                    <children>
                        <Label prefWidth="200.0" style="-fx-font-size: 14; -fx-text-fill: #7387A8;"
                               text="Selling Measurement" wrapText="true">
                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <JFXComboBox fx:id="sellingMeasurment" prefWidth="200.0"/>
                        <HBox fx:id="oneMeasurePriceHbox" prefHeight="30.0" spacing="11.0">
                            <children>
                                <Label style="-fx-font-size: 14; -fx-text-fill: #7387A8;"
                                       text="One Selling measure Price">
                                    <HBox.margin>
                                        <Insets top="10.0"/>
                                    </HBox.margin>
                                </Label>
                                <JFXTextField fx:id="oneMeasurePrice" prefWidth="100.0"
                                              style="-fx-background-color: #EBF5FB;">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </JFXTextField>
                            </children>
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                            <HBox.margin>
                                <Insets left="30.0"/>
                            </HBox.margin>
                        </HBox>
                    </children>
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="30.0" spacing="11.0">
                    <children>
                        <Label prefWidth="200.0" style="-fx-font-size: 14; -fx-text-fill: #7387A8;"
                               text="One unit to selling measurement">
                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <JFXTextField fx:id="unitToSellingMeasurment" prefWidth="200.0"
                                      style="-fx-background-color: #EBF5FB;">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </JFXTextField>
                        <DoubleValidator fx:id="unitSellingPriceDoubleValidator" layoutX="453.0" layoutY="10.0"/>
                        <Label alignment="CENTER" text="Has broker">
                            <font>
                                <Font size="14.0"/>
                            </font>
                            <HBox.margin>
                                <Insets left="50.0"/>
                            </HBox.margin>
                        </Label>
                        <JFXCheckBox fx:id="hasBroker" alignment="CENTER_RIGHT" text="." textAlignment="RIGHT"
                                     textOverrun="CLIP">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </JFXCheckBox>
                    </children>
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="30.0"/>
                    </VBox.margin>
                </HBox>
                <HBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
                    <children>
                        <ButtonBar buttonMinWidth="0.0">
                            <buttons>
                                <JFXButton fx:id="cancel" buttonType="RAISED" onAction="#cancelCloseWindow"
                                           prefWidth="100.0" ripplerFill="#81acee"
                                           style="-fx-border-width: 1; -fx-border-color: #81acee;" text="Cancel"
                                           textFill="#270dcd">
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                    <padding>
                                        <Insets bottom="8.0" left="5.0" right="5.0" top="8.0"/>
                                    </padding>
                                </JFXButton>
                                <JFXButton fx:id="resetInput" buttonType="RAISED" onAction="#delet" prefWidth="100.0"
                                           ripplerFill="#a2c2f6" style="-fx-background-color: #EC7063;" text="Delete"
                                           textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                    <padding>
                                        <Insets bottom="8.0" left="5.0" right="5.0" top="8.0"/>
                                    </padding>
                                </JFXButton>
                                <JFXButton fx:id="saveButton" buttonType="RAISED" onAction="#saveData" prefWidth="100.0"
                                           ripplerFill="#a2c2f6" style="-fx-background-color: #2d76eb;" text="Update"
                                           textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                    <padding>
                                        <Insets bottom="8.0" left="5.0" right="5.0" top="8.0"/>
                                    </padding>
                                </JFXButton>
                            </buttons>
                            <HBox.margin>
                                <Insets bottom="10.0"/>
                            </HBox.margin>
                        </ButtonBar>
                    </children>
                    <padding>
                        <Insets right="20.0" top="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0"/>
                    </VBox.margin>
                </HBox>
            </children>
            <padding>
                <Insets left="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
