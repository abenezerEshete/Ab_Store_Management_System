<?xml version="1.0" encoding="UTF-8"?>
<?import com.jfoenix.controls.*?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox nodeOrientation="LEFT_TO_RIGHT" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TotalProduct">
    <children>
        <JFXToolbar style="-fx-background-color: #FDFEFE;">
            <right>
                <VBox BorderPane.alignment="CENTER">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                    </padding>
                </VBox>
            </right>
            <left>
                <Label fx:id="countList" alignment="BOTTOM_LEFT" style="-fx-font-size: 18; -fx-font-weight: bold;" text="All Products" BorderPane.alignment="CENTER">
                    <padding>
                        <Insets left="30.0" top="30.0" />
                    </padding>
                </Label>
            </left>
            <center>
                <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                    <children>
                        <JFXTextField fx:id="searchText" minWidth="250.0" onAction="#productSearch" promptText="Search Product">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="10.0" />
                            </HBox.margin>
                        </JFXTextField>
                        <JFXButton fx:id="productTypeSearch" contentDisplay="GRAPHIC_ONLY" graphicTextGap="10.0" minWidth="40.0" onAction="#productSearch" prefWidth="30.0" ripplerFill="#90e8e6" style="-fx-background-color: #F4F6F6; -fx-background-radius: 15; -fx-border-radius: 15;" text="." textFill="WHITE">
                            <font>
                                <Font name="Roboto Medium" size="14.0" />
                            </font>
                            <graphic>
                                <FontAwesomeIconView fill="#00c0ef" glyphName="SEARCH" size="20" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
                            </graphic>
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </JFXButton>
                    </children>
                    <padding>
                        <Insets left="20.0" top="20.0" />
                    </padding>
                </HBox>
            </center>
        </JFXToolbar>
        <JFXTabPane prefWidth="500.0" />
        <Separator>
            <VBox.margin>
                <Insets bottom="3.0" />
            </VBox.margin>
        </Separator>
        <TableView fx:id="productTypeTable" style="-fx-font-size: 14;" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="productId" editable="false" prefWidth="75.0" resizable="false" sortable="false" text="id" visible="false" />
                <TableColumn fx:id="productNumber" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" resizable="false" text="no." />
                <TableColumn fx:id="productName" minWidth="200.0" prefWidth="250.0" text="Name" />
                <TableColumn fx:id="productType" minWidth="70.0" prefWidth="70.0" text="Type" />
                <TableColumn fx:id="productGrade" maxWidth="276.0" minWidth="70.0" prefWidth="70.0" sortable="false" text="grade" />
                <TableColumn fx:id="productModel" maxWidth="231.0" minWidth="70.0" prefWidth="70.0" text="Model" />
                <TableColumn fx:id="productSize" minWidth="70.0" prefWidth="70.0" sortable="false" text="Size" />
                <TableColumn fx:id="productBrand" minWidth="150.0" prefWidth="150.0" text="Brand" />
                <TableColumn fx:id="productAmount" minWidth="200.0" prefWidth="-1.0" text="Amount" />
            </columns>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </TableView>
        <HBox nodeOrientation="RIGHT_TO_LEFT" spacing="3.0">
            <children>
                <JFXButton fx:id="next" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" graphicTextGap="10.0" onAction="#nextload" ripplerFill="#756666" style="-fx-background-color: #00C0EF;" text="." textFill="WHITE">
                    <font>
                        <Font size="1.0" />
                    </font>
                    <graphic>
                        <FontAwesomeIconView fill="#fcf8f8" glyphName="ANGLE_RIGHT" size="30" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
                    </graphic>
                    <padding>
                        <Insets left="15.0" right="15.0" />
                    </padding>
                    <HBox.margin>
                        <Insets bottom="2.0" top="3.0" />
                    </HBox.margin>
                </JFXButton>
                <Label fx:id="itemPage" alignment="CENTER" minWidth="70.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #7387A8;" text="123">
                    <padding>
                        <Insets top="5.0" />
                    </padding>
                    <HBox.margin>
                        <Insets left="3.0" right="3.0" />
                    </HBox.margin>
                </Label>
                <JFXButton fx:id="back" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" graphicTextGap="10.0" onAction="#backpage" ripplerFill="#756666" style="-fx-background-color: #00C0EF;" text="." textFill="WHITE">
                    <font>
                        <Font size="1.0" />
                    </font>
                    <graphic>
                        <FontAwesomeIconView fill="#fcf8f8" glyphName="ANGLE_LEFT" size="30" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="INSIDE" />
                    </graphic>
                    <padding>
                        <Insets left="15.0" right="15.0" />
                    </padding>
                    <HBox.margin>
                        <Insets bottom="2.0" top="3.0" />
                    </HBox.margin>
                </JFXButton>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
